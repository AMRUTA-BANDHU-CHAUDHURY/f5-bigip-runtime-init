        "eslint-plugin-import": "^2.20.0",
        "@f5devcentral/eslint-config-f5-atg": "^0.1.1"
    },
    "eslintConfig": {
        "extends": "@f5devcentral/eslint-config-f5-atg",
        "rules": {
            "func-names": "off",
            "max-classes-per-file": [
                2,
                5
            ]
        },
        "parserOptions": {
            "ecmaVersion": 8
        }
